{
  "connectionString": "${file:/mnt/secrets/ccpay/app-insights-connection-string}",
  "role": {
    "name": "payment-app"
  },
  "sampling": {
    "overrides": [
      {
        "telemetryType": "request",
        "attributes": [
          {
            "key": "url.path",
            "value": "/health.*",
            "matchType": "regexp"
          }
        ],
        "percentage": 1
      }
    ]
  },
  "instrumentation": {
    "micrometer": {
      "enabled": false
    }
  }
}
